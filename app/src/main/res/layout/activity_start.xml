<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="20dp"
    android:paddingTop="20dp"
    android:weightSum="1"
    tools:context="com.example.lenovo.anitaborgregister.start"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_weight="0.08"
        android:layout_x="12dp"
        android:layout_y="65dp"
        android:text="First Name"
        android:textStyle="bold|italic"
        tools:fontFamily="sans-serif-condensed"
        tools:layout_editor_absoluteY="3dp"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="73dp"
        android:layout_height="35dp"
        android:layout_weight="0.08"
        android:layout_x="11dp"
        android:layout_y="157dp"
        android:text="Last Name"
        android:textStyle="bold|italic"
        tools:fontFamily="sans-serif-condensed"
        tools:layout_editor_absoluteY="83dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="95dp"
        android:layout_height="38dp"
        android:layout_weight="0.08"
        android:layout_x="19dp"
        android:layout_y="392dp"
        android:text=" Email-id"
        android:textStyle="bold|italic"
        tools:fontFamily="sans-serif-condensed"
        tools:layout_editor_absoluteY="154dp"
        tools:layout_editor_absoluteX="-4dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.08"
        android:layout_x="128dp"
        android:layout_y="69dp"
        android:ems="10"
        android:hint="Eg. John"
        android:inputType="textPersonName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="1dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.08"
        android:layout_x="128dp"
        android:layout_y="69dp"
        android:ems="10"
        android:hint="Eg. Doe "
        android:inputType="textPersonName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="81dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.08"
        android:layout_x="128dp"
        android:layout_y="69dp"
        android:ems="10"
        android:hint="Eg. 0123456789"
        android:inputType="textPersonName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="234dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.08"
        android:layout_x="128dp"
        android:layout_y="69dp"
        android:ems="10"
        android:hint="Eg. johndoe@domain.com"
        android:inputType="textPersonName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="154dp" />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_weight="0.08"
        android:layout_x="31dp"
        android:layout_y="245dp"
        android:text="Contact no."
        android:textStyle="bold|italic"
        tools:fontFamily="sans-serif-condensed"
        tools:layout_editor_absoluteY="238dp"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="MissingConstraints" />
</android.support.constraint.ConstraintLayout>
